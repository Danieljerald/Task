@model IEnumerable<FeedbackModel>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Feedback</title>
</head>
<body>
    <h2 style="text-align: center;">Feedback's</h2><br>
    <a asp-controller="Admin" asp-action="clearFeedbacks" ><input type="button" value="Clear All" style="margin-left:90%" class="btn btn-danger"></a>
    <br>

    <table class="table">
        <thead>
            <tr> 
                <th scope="col">Username</th>
                <th scope="col">Message</th>
            </tr>
        </thead>
        <tbody>
        @foreach (var item in Model)
        {
            <tr>
            <td>@item.username</td>
            <td>@item.comment</td>
            </tr>
        }
  </tbody>
    </table>
</body>
</html>